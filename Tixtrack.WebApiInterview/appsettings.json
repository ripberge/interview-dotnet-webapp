{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      },
      "Using": [
        "Serilog.Sinks.File"
      ]
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day",
          "buffered": true
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
